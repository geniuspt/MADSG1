<h1>Listing projs</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @projs.each do |proj| %>
  <tr>
    <td><%= proj.name %></td>
    <td><%= link_to 'Show', proj %></td>
    <td><%= link_to 'Edit', edit_proj_path(proj) %></td>
    <td><%= link_to 'Destroy', proj, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

